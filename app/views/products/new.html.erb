
<div>
  <%= form_for(@product) do |f| %>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
    <%= f.label :detail %><br>
    <%= f.text_area :detail %>
    <%= f.label :link %><br>
    <%= f.text_field :link %>
    <%= f.label :image %><br>
    <%= f.file_field :image %>
    <%= f.submit %>
  <% end %>
</div>

